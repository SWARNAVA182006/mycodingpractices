#include<iostream>
using namespace std;
int main(){
    int n,x=0;
    cout<<"Enter the n value= ";
    cin>>n;
    for(int i=1;i<=n;++i){
      
    if(n%i==0){
        cout<<i<<endl;
        x+=1;
    }else{
        continue;
    }
    }
    if(x==2){
        cout<<n<<" is a prime number.";
    }else{
        cout<<n<<" is not a prime number.";
    }
    return 0;
}
